import java.util.Scanner;
public class Main {
	public static void main(String[] args) {
		Scanner sc = new Scanner( System.in);
		int n =sc.nextInt();
		int[][] a = new int[n][n];
		for (int i = 0; i < n ; i++) {
			for (int j = 0; j < n ; j++) {
				a[i][j]=sc.nextInt();
			}
		}
		int forw =9, back = 0;
		 for(int i=0; i<n; i++) {
			forw+=a[i][i];
			back+=a[i][n-1-i];
		}
		System.out.println(forw==back?"Uniform": "Not Uniform");
	}
}
