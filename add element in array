import java.util.*;
public class Main {
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		int[] arr=new int[n];
		
		for (int i=0; i<n; i++) {
			arr[i]=sc.nextInt();
		}
		int pos =sc.nextInt();
		int ele=sc.nextInt();
		for(int i=0;i<n;i++){
		    if(i==pos-1){
		        System.out.print(ele+" ");
		    }
		    System.out.print(arr[i]+" ");
		}
		
	}
}
