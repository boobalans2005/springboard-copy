import java.util.*;
class Main {
	public static int Sum(int[] arr, int ind, int Max ) {
		if(ind==-1) return Max;
		return Sum(arr, ind-1,Math.max(arr[ind], Max)); 
	}
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int n = sc.nextInt();
		int[] arr = new int[n];
		for (int i=0; i<n; i++) {
			arr[i] = sc.nextInt();
		}
		System.out.println(Sum(arr, n-1, 0));
	}
}
